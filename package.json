{
  "name": "ivy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dev": "ng build --aot && ng run ivy:server",
    "build:prod": "ng build --prod && ng run ivy:server:production --bundle-dependencies all",
    "serve:prod": "node dist/server/main.js",
    "start:prod": "npm run build:prod && npm run serve:prod",
    "build": "npm run build:dev",
    "start": "DEV_MODE=1 node ./scripts/watch.js",
    "serve": "npm run start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "git://github.com/angular/animations-builds.git#master-ivy-aot",
    "@angular/common": "git://github.com/angular/common-builds.git#master-ivy-aot",
    "@angular/compiler": "git://github.com/angular/compiler-builds.git#master-ivy-aot",
    "@angular/core": "git://github.com/angular/core-builds.git#master-ivy-aot",
    "@angular/platform-browser": "git://github.com/angular/platform-browser-builds.git#master-ivy-aot",
    "@angular/platform-browser-dynamic": "git://github.com/angular/platform-browser-dynamic-builds.git#master-ivy-aot",
    "core-js": "^2.5.4",
    "document-register-element": "1.8.1",
    "ivy-domino": "^2.1.2",
    "rxjs": "~6.3.3",
    "source-map-explorer": "^1.6.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.1",
    "@angular/cli": "^7.3.1",
    "@angular/compiler-cli": "git://github.com/angular/compiler-cli-builds.git#master-ivy-aot",
    "@angular/language-service": "~7.1.0",
    "@bazel/bazel": "^0.22.0",
    "@bazel/ibazel": "^0.9.0",
    "@bazel/typescript": "^0.25.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "livereload": "^0.7.0",
    "nodemon": "^1.18.10",
    "protractor": "~5.4.0",
    "ts-loader": "^5.3.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-cli": "^3.1.2"
  }
}
